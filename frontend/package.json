    "react-window": "^1.8.8",
    "fuse.js": "^7.0.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "immer": "^10.0.3",
    "comlink": "^4.4.1",
    "workbox-window": "^7.0.0",
    "idb": "^8.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^9.0.7",
    "@types/react-window": "^1.8.8",
